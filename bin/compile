#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir>

set -e
set -o pipefail

BUILD_DIR=$1
CACHE_DIR=$2

ls -a

echo "-----> BUILD_DIR="$1
echo "-----> CACHE_DIR="$2

cd $1


echo "-----> Installing"
pwd
echo $(find . -maxdepth 1 -type d -name "*" ! -name "." !-name "PCube")
echo "-----> rep deleted"
echo $(find . -maxdepth 1 -type f -name "*")